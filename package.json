{
  "name": "baseline-flightdeck",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "lint": "turbo lint",
    "scan:example": "pnpm -C packages/cli start scan --src ./examples/sample-app --report ./examples/sample-app/.baseline --analytics ./examples/sample-app/traffic.csv",
    "web:dev": "pnpm -C packages/web-report dev",
    "web:export": "pnpm -C packages/web-report export",
    "fetch:features": "node ./scripts/fetch-web-features.mjs",
    "release:radar": "node ./scripts/release-radar.mjs",
    "docker:up": "docker compose -f docker/docker-compose.yml up --build"
  },
  "devDependencies": {
    "turbo": "^2.1.1",
    "typescript": "^5.6.3"
  }
}
